<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="Cabbyx" location="startMenu"/>
	<shortcut
		name="Cabbyx Server"
		target="$INSTALL_PATH\Cabbyx.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start Cabbyx server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="Cabbyx Wallet"
		target="http://localhost:6876/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch Cabbyx wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>